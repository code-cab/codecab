{
  "name": "codecab",
  "version": "2.1.1",
  "author": "Maarten Krijgsman",
  "main": "src/index.js",
  "dependencies": {
    "pixi.js": "^4.8.3",
    "bit-twiddle": "^1.0.2",
    "colorsys": "^1.0.20",
    "earcut": "^2.0.7",
    "eventemitter3": "^2.0.0",
    "ismobilejs": "^0.4.0",
    "json-loader": "^0.5.7",
    "object-assign": "^4.0.1",
    "pixi-filters": "^2.6.0",
    "pixi-gl-core": "^1.1.4",
    "pixi-sound": "^2.0.1",
    "webfontloader": "latest",
    "remove-array-items": "^1.0.0",
    "resource-loader": "^2.2.2",
    "tween.js": "^16.6.0",
    "poly-to-pslg": "^1.0.1",
    "cdt2d": "^1.0.0",
    "js-clipper": "^1.0.1",
    "webworkify": "^1.5.0",
    "lucy-b2": "^1.0.1"
  },
  "devDependencies": {
    "gulp": "^3.9.1",
    "browserify": "^15.2.0",
    "browserify-replace": "^0.9.0",
    "browserify-livereload": "^1.0.10",
    "replace-x": "^1.5.0",
    "watch": "^1.0.2",
    "brfs-babel": "^1.0.0",
    "vinyl-fs": "^3.0.1",
    "vinyl-buffer": "^1.0.0",
    "eslint": "^4.15.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-static-fs": "^1.1.0",
    "babel-plugin-version-inline": "^1.0.0",
    "babel-preset-env": "^1.6.1",
    "babelify": "^8.0.0",
    "rimraf": "^2.6.2",
    "mkdirp": "^0.5.1",
    "typescript": "^2.6.2",
    "watchify": "^3.9.0",
    "cpy-cli": "^2.0.0",
    "mocha": "^5.2.0",
    "mochawesome": "^3.0.2"
  },
  "scripts": {
    "test": "scripts/build",
    "clean": "rimraf build && rimraf generate && mkdirp generate/lib && mkdirp build",
    "typescriptlib.turbulenz": "tsc lib/ts/turbulenz/index.ts --module ES2015 --target ES2015 --outDir generate/lib",
    "typescriptlib": "npm run typescriptlib.turbulenz",
    "browserify": "browserify src/windowscope.js -o build/codecab-%npm_package_version%.js -t [ brfs-babel ] -t [ babelify ]",
    "browserify-linux": "browserify src/windowscope.js -o build/codecab-$npm_package_version.js -t [ brfs-babel ] -t [ babelify ]",
    "watch": "watchify -p [ browserify-livereload --host 127.0.0.1 --port 1337 ] src/windowscope.js -o build/codecab.js --verbose -t [ brfs-babel ] -t [ babelify ]",
    "watch2ide": "watchify -p [ browserify-livereload --host 127.0.0.1 --port 1337 ] src/windowscope.js -o ../codecab-ide/build/codecab-%npm_package_version%.js --verbose -t [ brfs-babel ] -t [ babelify ]",
    "copy2ide": "cpy build/codecab-%npm_package_version%.js ../codecab-ide/build",
    "build": "npm run clean && npm run typescriptlib && npm run browserify",
    "build-linux": "npm run clean && npm run typescriptlib && npm run browserify-linux"
  }
}
